    <AssemblyTitle>Dreamhost API Client</AssemblyTitle>
    <Company>Eric Hobbs</Company>
    <Product>Dreamhost.Api</Product>
    <Description>Simple Dreamhost Api Library</Description>
    <Copyright>Copyright  Eric Hobbs © 2016</Copyright>
    <OutputPath>..\bin\$(Configuration)\</OutputPath>
